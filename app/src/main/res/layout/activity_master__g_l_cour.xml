<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Master_GL_cour">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="240dp"
        tools:layout_editor_absoluteY="212dp"
        android:background="#d9e8ee">

        <TextView
            android:id="@+id/master1_GL"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            style="@style/CategoryStyle"
            android:backgroundTint="#2E8BC0"
            android:background="@drawable/raduis "
            android:text="Mastr 1 GL" />

        <TextView
            android:id="@+id/master2_GL"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            style="@style/CategoryStyle"
            android:backgroundTint="#2E8BC0"
            android:background="@drawable/raduis "
            android:text="Master 2 GL" />
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>